{
	"name": "local-cloud",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"init": "pxio",
		"start": "pxio start",
		"build": "pxio build",
		"prepare-dist": "cd ./dist && mkdir temp && mv ./package.json ./temp && mv ./server ./temp/server && cd temp && npm i",
		"compile": "cd dist/temp && pkg . --target latest-linux-x64 --output ./../local-cloud",
		"dist": "npm run build && npm run prepare-dist && npm run compile && cd dist && rm -R ./temp && curl -fsSL https://esbuild.github.io/dl/v0.23.1 | sh"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"esbuild": "^0.23.1",
		"express": "^4.18.3",
		"express-fileupload": "^1.5.0",
		"express-session": "^1.17.3",
		"ini": "^4.1.2",
		"liquidjs": "^10.9.3",
		"netjet": "^1.4.0",
		"spdy": "^4.0.2",
		"sqlite3": "^5.1.6",
		"unzipper": "^0.10.14",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@ionic/core": "^7.5.3",
		"@types/compression": "^1.7.4",
		"@types/express": "^4.17.21",
		"@types/express-fileupload": "^1.5.0",
		"@types/express-session": "^1.17.9",
		"@types/ini": "^4.1.0",
		"@types/node": "^20.11.29",
		"@types/spdy": "^3.4.9",
		"@types/unzipper": "^0.10.9",
		"@types/uuid": "^9.0.6",
		"pkg": "^5.8.1",
		"px.io": "^0.12.17-beta"
	},
	"pxio": {
		"type": "http",
		"boot": "manual",
		"resources": [
			"connector",
			"lc"
		],
		"loader": {
			".sql": "text"
		}
	}
}
